<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:layout_marginTop="@dimen/margine"
    android:layout_marginBottom="@dimen/margine"
    android:layout_marginLeft="@dimen/margine"
    android:layout_marginRight="@dimen/margine"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/biasa"
        android:layout_marginBottom="@dimen/margine"
        android:text="Intent Biasa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/dgn_data"
        android:layout_marginBottom="@dimen/margine"
        android:text="Intent Dengan Data"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/dgn_obyek"
        android:layout_marginBottom="@dimen/margine"
        android:text="Intent Dengan Objek"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/implicit"
        android:layout_marginBottom="@dimen/margine"
        android:text="Implicit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/gondol"
        android:layout_marginBottom="@dimen/margine"
        android:text="Intent Dengan Kembalian"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_gravity="center"
        android:text="Oke"
        android:id="@+id/hasil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>